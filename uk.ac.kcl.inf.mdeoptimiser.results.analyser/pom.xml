            <artifactId>moeaframework</artifactId>
            <version>2.12</version>
        </dependency>
        <dependency>
            <groupId>uk.ac.kcl.inf.mdeoptimiser.languages</groupId>
            <artifactId>uk.ac.kcl.inf.mdeoptimiser.languages</artifactId>
            <version>1.0.0-SNAPSHOT</version>
