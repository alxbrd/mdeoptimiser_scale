    </parent>
    <modelVersion>4.0.0</modelVersion>

    <groupId>uk.ac.kcl.inf.mdeoptimiser.results</groupId>
    <artifactId>uk.ac.kcl.inf.mdeoptimiser.results.analyser</artifactId>

    <build>
        <plugins>
            <plugin>
                <groupId>com.coveo</groupId>
                <artifactId>fmt-maven-plugin</artifactId>
            </plugin>
